<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="WhoScoredXMLPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>whoscored.data.Club</class>
    <class>whoscored.data.PlayerMatchStatistic</class>
    <class>whoscored.data.Match</class>
    <class>whoscored.data.Player</class>
    <class>whoscored.data.YellowCard</class>
    <class>whoscored.data.TeamMatchStatistic</class>
    <class>whoscored.data.Goal</class>
    <class>whoscored.data.League</class>
    <class>whoscored.data.RedCard</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/XMLProject"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="worldcup@2022"/>
    </properties>
  </persistence-unit>
</persistence>
